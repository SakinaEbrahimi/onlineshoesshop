<%- include('../include/header.ejs') %>
    </header>
    <!-- Header Section End -->
    
    <!-- Categories Section Begin -->
    <section class="categories">
        <div class="container">
            <div class="row">
                <div class="categories__slider owl-carousel">
                    <% if(women.length>0){
                        
                        for(p of women){
                           
                            %>
                    <div class="col-lg-3">
                        <div class="categories__item set-bg" >
                            <img src="/<%= p.imageUrl%>" alt="">
                            <h5><a href="#"><%= p.title%></a></h5>
                        </div>
                    </div>
                    <%}}%>
                </div>
            </div>
        </div>
    </section>
    <!-- Categories Section End -->

    <!-- Featured Section Begin -->
    <section class="featured spad">
        <div class="container">
            <div class="col">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Women Best Shoes</h2>
                    </div>
                </div>
            </div>
            <div class="row featured__filter">
                <% if(women.length>0){
                        
                    for(w of women){
                       
                        %>
                <div class="col-lg-4 col-md-3 col-sm-6 mix oranges fresh-meat">
                    <div class="featured__item ">
                        <div class="featured__item__pic set-bg">
                            <div class="col-lg-10 col-sm-11 col-md-12" style="justify-content: center; margin: 0ch auto;">
                                <div class="categories__item set-bg" >
                                    <img src="/<%= w.imageUrl%>" alt="">
                                </div>
                            </div>
                            <ul class="featured__item__pic__hover">
                                <li><a href="#"><i class="fa fa-heart"></i></a></li>
                                <li><a href=""><i class="fa fa-retweet"></i></a></li>
                                
                            </ul>
                        </div>
                        
                        <div class="featured__item__text">
                            <h6><%= w.title%></h6>
                            <h5><%= w.price%>$</h5>
                        </div>
                        <p style="color: rgb(202, 100, 100); text-align: center;">Color : <%= w.color%></p>                            
                        <p style="color: rgb(130, 201, 219); text-align: center;">OFF : <%= w.off%> %</p>   
                        <div class="featured__item__text">
                        
                            <a href="/detail/<%=w._id%>" style="color: black; text-align: center; background-color: rgb(109, 193, 196);">...More Details..</a>
                        </div>                         
                    </div>
                </div>
                <%}}%>
            
    
            </div>
        </div>
    </section>
    <!-- Featured Section End -->
    <!-- Pagination# -->
    <%- include('../include/pagination.ejs',{
        currentPage: currentPage,
        nextPage: nextPage,
        prePage: prePage,
        lastPage: lastPage  ,
        hasNextPage: hasNextPage,
        hasPrePage: hasPrePage
    })%>   
    
    <!-- Banner Start -->
    <%- include('../include/banner.ejs')%>

    <!-- Latest Product Section Start -->
    <%- include('../include/topProduct.ejs')%>

    <%- include('../include/end.ejs')%>